{
  "name": "videoeditor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "lint": "npm run lint:script && npm run lint:style",
    "lint-fix": "npm run lint:script-fix && npm run lint:style-fix",
    "lint:script": "eslint --ext .js,.jsx,.ts,.tsx,.vue --quiet ./",
    "lint:script-fix": "eslint --ext .js,.jsx,.ts,.tsx,.vue --fix --quiet ./",
    "lint:style": "stylelint \"./**/*.{css,scss,sass,vue,html}\"",
    "lint:style-fix": "stylelint \"./**/*.{css,scss,sass,vue,html}\" --fix",
    "lint-staged": "lint-staged",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "element-plus": "^2.3.4",
    "mockjs": "^1.1.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@types/node": "^20.1.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-vue": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.12.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "stylelint": "^15.6.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-scss": "^11.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-prettier": "^3.0.0",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.2",
    "vite-plugin-mock": "^2.9.8 ",
    "vue-eslint-parser": "^9.3.0",
    "vue-tsc": "^1.4.2"
  }
}
